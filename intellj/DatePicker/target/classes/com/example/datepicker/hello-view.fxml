<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="284.0" prefWidth="364.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.datepicker.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ChoiceBox fx:id="animals" prefWidth="150.0" />
   <DatePicker fx:id="datePicker" />
    <Button fx:id="getDate" onAction="#onHelloButtonClick" onMouseClicked="#getDate" text="Get Date" />
</VBox>
