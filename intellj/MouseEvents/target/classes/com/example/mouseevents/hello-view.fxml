<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="256.0" prefWidth="342.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mouseevents.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ColorPicker fx:id="color" />
    <Button fx:id="button01" onAction="#onHelloButtonClick" onMouseClicked="#Hide" onMouseEntered="#Enter" text="Click me" />

    <Label fx:id="welcomeText" prefHeight="21.0" prefWidth="155.0" text="Text" textFill="#cd2323" underline="true">
      <font>
         <Font size="36.0" />
      </font></Label>
</VBox>
